<!-- https://www.matuzo.at/blog/html-boilerplate/ -->
<!doctype html>
<html lang="en" prefix="og: http://ogp.me/ns#">
    <head>
        {% include "_head.njk" %}
    </head>
    <body>
        <header>
            {% include "_header.njk" %}
        </header>
        <main>
            {% include "_postHeader.njk" %}

            <div id="content">
                {%- if content | toc | safe != "" -%}
                <div class="tableOfContents">
                    <h2>Contents</h2>
                    {{content | toc | safe}}
                </div>
                <hr>
                {%- endif -%}

                {{ content | safe }}
            </div>
        </main>
    </body>
</html>